<div class="mContainerPageContent">
  <div class="mContainerSearchBlock">
    <div class="mContainerSearchInput">
      <i class="fa fa-search mContainerSearchIcon" aria-hidden="true"></i>
      <input type="text" pInputText (click) ="mcontainer.toggle($event)" class="mContainerSearchInputBlock" placeholder="                                     Search         " />
    </div>
  </div>
  <div class="mContaineroverlayPanel">
    <p-overlayPanel #mcontainer modal="false">
    <div class="mContainerSearchOption">
      <div class="mContainerSearchOptionDiv" (click)="mcontainer.hide($event)">
        <div class="mContainerPickCarrier">PickUpFromCarrier</div>
        <div class="mContainerBlackSearch">&nbsp;</div>
        <div class="mContainerPickAnyBlock">Select
          <i class="fa fa-angle-right" aria-hidden="true"></i>
        </div>
      </div>
      <div class="mContainerSearchOptionDiv" (click)="mcontainer.hide($event)">
        <div class="mContainerPickReserved">Reserved</div>
        <div class="mContainerPickAnyBlock">Select
          <i class="fa fa-angle-right" aria-hidden="true"></i>
        </div>
      </div>

      <div class="mContainerSearchOptionDiv" (click)="mcontainer.hide($event)">
        <div class="mContainerPickSize">ContainerSize</div>
        <div class="mContainerPickAnyBlock">Select
          <i class="fa fa-angle-right" aria-hidden="true"></i>
        </div>
      </div>
    </div>
    <div class="mContainerSearchHandlerBlock">
      <div class="mContainerCancelButton" (click) = "mcontainer.hide($event)">Search</div>
    </div>
  </p-overlayPanel>
  </div>
  <div class="mContainerReserveBlock">
    <div class="mContainerReserveButtonBlock">
      <div class="mContainerReserveButton">
        Reserve for Stuffling
      </div>
    </div>
    <div class="mContainerReserveIconBlock">
      <div class="mContainerReservePencilIcon"><i class="fa fa-pencil-square-o mContainerReservePencil" aria-hidden="true"></i></div>
      <div class="mContainerReserveTrashIcon"><i class="fa fa-trash-o mContainerReserveTrash" aria-hidden="true"></i></div>
    </div>
  </div>
  <div class="mContainerListBlock">
    <div class="mContainersDetailsBlock">
      <div class="mContainerRedCircle"></div>
      <div class="mContainerNumber">UACU5936899</div>
      <div class="mContainerName">
        <div class="mContainercarrierBlock">carrier</div>
        <div class="mContainerOOCLBlock">OOCL</div>
      </div>
      <div class="mContainerPickupDate">15 FEB 2017-PickUp</div>
    </div>
    <div class="mContainersStatusBlock">
      <div class="mContainersStatus">
        <div><i class="fa fa-check-square-o mcontainersCheck" aria-hidden="true"></i></div>
        <div class="mContainersReserved">Reserved</div>
      </div>
    </div>
  </div>
  <div class="mContainerListBlock" *ngFor="let container of dummyArray; let i = index;">
    <div class="mContainersDetailsBlock">
      <div class="mContainerNumber">UACU5936899</div>
      <div class="mContainerName">
        <div class="mContainercarrierBlock">carrier</div>
        <div class="mContainerOOCLBlock">OOCL</div>
      </div>
      <div class="mContainerPickupDate">25 FEB 2017-WHS Gate-In</div>
    </div>
    <div class="mContainersStatusBlock">
      <div class="mContainersStatus">
        <div><i class="fa fa-check-square-o mcontainersCheck" aria-hidden="true"></i></div>
        <div class="mContainersReserved">N Reserved</div>
      </div>
    </div>
  </div>
</div>
