@import '../../styles.scss';

//Stand Classes

.ui-inputtext{
  padding: 10px !important;
  width: 85% !important;
}

//Component Classes

.mContainersPageContent{
  min-height: 344px;
  padding-top: 52px;
}

.mContainersMainDiv{
  background: #c2c2d0;
  height: 35px;
  margin: -8px;
  padding: 14px;
}

.mContainersSearchBlock{
margin-top: -1%;
}

.mContainersSearchInput{
  width: 70%;
  float: left;
}

.mContainersSearchButton{
  width: 30%;
  float: left;
  border-radius: 6px;
  margin-top: -6px;
  margin-left: -24px;
}

.mContainersButton{
  background: $emaroon;
}

.mContainersButton .ui-button, button.ui-button.ui-state-default, .ui-button.ui-state-default{
    background: $edarkgray;
    border: 1px solid $edarkgray;
    padding: 6%;
    margin-left: 25%;
    margin-top: 7.5%;
    z-index: 10;
    position: inherit;
}

