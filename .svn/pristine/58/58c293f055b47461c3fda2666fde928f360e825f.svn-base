}

.loginButton{
  margin-left: 23px;
  width:85%;
}
.loginInput:focus { outline: none; }


 Label 

.loginLabel{
	color: $egrayfieldlabel;
	font-size: 12px;
	font-weight: normal;
	position: absolute;
  margin-left: 17px;
	pointer-events: none;
	left: 5px;
	top: 10px;
	-webkit-transition:all 0.2s ease;
	transition: all 0.2s ease;
}

 active 
.loginInput:focus ~ .loginLabel, .loginInput.used ~ .loginLabel {
	top: -10px;
  font-size: 14px;
  color: $egrayfieldlabel;
  -webkit-transform: scale(.85);
          transform: scale(.85); left:0.35px;

}




 Underline 
.bar {
	position: relative;
	display: block;
	width: 85%;
  // background-color:gray;
}



.bar:before, .bar:after {
	content: '';
	height: 2px;
	width: 0;
	bottom: 1px;
  margin-left: 23px;
	position: absolute;
	background: $emaroon;
	-webkit-transition:all 0.2s ease;
	transition: all 0.2s ease;
}

.bar:before { left: 50%; }

.bar:after {
  left:0%; }


 active 

input:focus ~ .bar:before, input:focus ~ .bar:after { width: 50%; }





 active 
.loginInput:focus ~ .bar:before {
  width: 50%;
  color: $egrayfieldlabel;

}



 Highlight 

.highlight {
	position: absolute;
	height: 60%;
	width: 100px;
	top: 25%;
	left: 0;
	pointer-events: none;
	opacity: 0.5;
}


 active 
.loginInput:focus ~ .highlight {
	-webkit-animation: inputHighlighter 0.3s ease;
	        animation: inputHighlighter 0.3s ease;
}


 Button 

.signInButton {
  position: relative;
  display: inline-block;
  padding: 12px 24px;
  margin: .3em 0 1em 0;
  width: 100%;
  vertical-align: middle;
  color: #fff;
  font-size: 13px;
  line-height: 20px;
  -webkit-font-smoothing: antialiased;
  text-align: center;
  letter-spacing: 1px;
  border: 0;

  cursor: pointer;
	background: $emaroon;
}



.footer{
	text-align: center;
	color: white;
	font-size: 13px;
	letter-spacing: .4px;
}
