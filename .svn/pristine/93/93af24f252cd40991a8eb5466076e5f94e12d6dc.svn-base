@import '../styles.scss';

//Standard Classes


//Component Classes

.mainBody {
  margin: -8px;
}

.mainHeader {
  height: 50px;
  background: $emaroon;
  color: $ewhite;
}

.headerBlock1 {
  float: left;
  width: 15%;
  margin-top: 3px;
}

.headerLogo {
  float: left;
  width: 44px;
  height: 44px;
  line-height: 44px;
  text-align: center;
  font-size: 18px;
}

.toolbarTitle {
  float: left;
  margin-right: 35px;
  font-size: 22px;
  font-weight: bold;
  line-height: 44px;
  font-family: 'Roboto' !important;
}

.headerBlock2 {
  border-radius: 25px;
  float: left;
  height: 34px;
  margin-top: 8px;
  width: 38%;
  background: $ewhite;
}

.headerSubBlock1 {
  background: $egrayfield;
  color: $egraymenulabel;
  float: left;
  width: 20%;
  height: 100%;
  border-radius: 25px 0px 0px 25px;
  text-align: center;
}

.headerSubBlock2 {
  float: left;
  width: 15%;
  // border: 1px solid $egraymenulabel;
}

.headerSubBlock3 {
  float: right;
  height: 70%;
  border: 5px solid white;
  border-radius: 25px;
  width: 15%;
  // background: $emaroon;
  // text-align: center;
}

.headerRetireveBtn {
  // float: right;
  height: 100%;
  border: none;
  border-radius: 25px;
  width: 100%;
  background: $emaroon;
  text-align: center;
  color: $ewhite;
  cursor: pointer;
}

.headerBlock3 {
  float: right;
  height: 30px;
  margin-top: 10px;
}

.headerIconBlock {
  height: 100%;
  line-height: 32px;
  width: 30px;
  margin-right: 15px;
  border-radius: 50%;
  background: $ewhite;
  color: $egraymenulabel;
  float: left;
  text-align: center;
  cursor: pointer;
}

.sideMenuBar {
  background: $egrayfield;
  color: $egraymenulabel;
  width: 5%;
  height: auto;
  float: left;
}

.sideMenuToogleBlock {
   padding-top: 20px;
   width: 99%;
   padding-bottom: 20px;
}

.sideMenuToogle {
  width: 50%;
  height: 25px;
  float: right;
  text-align: center;
  color: $egraymenulabel;
  background: rgb(220,220,220);
  border-radius: 25px 0px 0px 25px;
  cursor: pointer;
}


.sideMenuItems {
  // border: 1px solid black;
  padding-top: 13px;
  padding-bottom: 13px;
  color: $egraymenulabel;
  text-align: center;
  font-size: 20px;
  cursor: pointer;
}

.mainPageContent {
  float: left;
  width: 94.8%;
}
//SlideOut Css
.slideoutShowLeft {
  opacity: 1;
  left: 68px;
  overflow: auto;
  position: fixed;
  width: 85%;
  z-index: 9999;
  top: 50px;
  height: 100%;
  -webkit-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
}

.slideoutHideLeft {
  opacity: 1;
  left: -80%;
  overflow: auto;
  position: fixed;
  width: 85%;
  z-index: 99;
  top: 50px;
  height: 100%;
  -webkit-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
}

.slideoutShowRight {
  opacity: 0.5;
  left: 85%;
  overflow: auto;
  position: fixed;
  width: 15%;
  z-index: 9999;
  top: 50px;
  height: 100%;
  -webkit-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
}

.slideoutHideRight {
  opacity: 0.5;
  left: -15%;
  overflow: auto;
  position: fixed;
  width: 15%;
  z-index: 99;
  top: 50px;
  height: 100%;
  -webkit-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  box-shadow: 3px 0 6px rgba(0, 0, 0, 0.3);
  -webkit-transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  transition-duration: 0.3s;
}

.sideMenuHeaderBlock {
  text-align: center;
  top: 50px;
  left: 0;
  width: 100%;
  border-bottom: 1px solid white;
  // padding: 7px;
  padding-bottom: 2%;
}

.sideMenuTitle {
  color: rgb(102, 102, 102);
  font-size: 23px;
  font-weight: bolder;
  margin-top: 4%;
}

.toggleMenuBtn {
  margin-right: -30%;
  font-size: 20px;
  width: 100%;
  height: 100%;
  margin-top: 10%;
  color: black;
}

.menuList {
  padding: 13px;
  color: black;
  // border-bottom: .000001px solid #6f5151;
  border-bottom: .000001px solid #674747;

}



.toolbarRightMenu {
  float: right;
  display: block;
  margin: 0;
  padding: 0;
}

.toolbarLeftMenu {

  float: left;
  display: block;
  margin: 0;
  padding: 0;
}

.searchMenuBlock {
   background: $ewhite;
   border-radius: 25px;

}

.toolbarMenuTitleBlock {
  position: relative;
  display: block;
  float: left;
  cursor: pointer;
}

.toolbarMenuTitleBlock1 {
  height: 34px;
  width: 34px;
  position: relative;
  display: block;
  float: left;
  cursor: pointer;
  border-radius: 50%;
  background: white;
  color: $egraymenulabel;
}

.toolbarMenuTitle {
  display: block;
  height: 44px;
  padding: 0 10px;
  color: $egraymenulabel;
  line-height: 44px;
  text-decoration: none;
}

.toolbarMenuTitleFont {
  font-size: 13px;
  font-weight:bold;
}

.dropdownMenu {
  position: absolute;
  top: 100%;
  left: 0;
  z-index: 1000;
  display: none;
  float: left;
  min-width: 160px;
  padding: 5px 0;
  margin: 2px 0 0;
  list-style: none;
  font-size: 13px;
  text-align: left;
  background-color: #fff;
  border: 1px solid #ccc;
  border: 1px solid rgba(0, 0, 0, 0.15);
  border-radius: 0;
  -webkit-box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175);
  box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175);
  background-clip: padding-box;
}

.subToolbar {
  height: 65px;
  padding-top: 10px;
  padding-right: 16px;
  padding-bottom: 10px;
  padding-left: 16px;
  background-color: white;
  border-bottom: 1px solid #a8a8a8;
}

.subToolbarBox {
  width: 49.85%;
  float: left;
  height: 50%;
}




.mainContentBlock {
  width: 1200px;
  background-color: white;
  margin-left: calc((100% - 1200px)/2);
  height: 510px;
}

.contentHeader {
  background-color: #e2e2e0;
  height: 50px;
}

.contentToolbarTitle {
  text-align: center;
  color: #666666;
  font-size: 22px;
  font-weight: bold;
  line-height: 44px;
  font-family: 'Roboto' !important;
}

.displayMenuBelowBlock {
  position: relative;
  display: block;
  float: none;
}

.displayMenuBelow {
  display: block;
  font-size: 13px;
  font-weight:bold;
  height: 44px;
  padding: 0 10px;
  color: white;
  line-height: 44px;
}

.mContainerHeader{
  background: $emaroon;
  height: 50px;
  margin: -8px;
}

.mContainerHeaderBlanck{
  width: 20%;
  float: left;
}

.mContainerHeaderButtons{
  width: 40%;
  float: left;
  text-align: center;
  margin-top: 3%;
  margin-left: 5%;
}

.mContainerLoggedUser{
  width: 30%;
  float:left;
  text-align: right;
}

.mContainerUserIcon{
  text-align: center;
  margin-top: 8%;
  font-size: 180%;
}

.mContainerHeaderContainerButton{
  border: 1px solid black;
}

.mContainerHeaderBookingButton{
  border: 1px solid black;
}



.mContainerHeaderContainerButton{
  float: left;
  background: $ewhite;
  padding:3px;
}

.mContainerHeaderBookingButton{
  float: left;
  padding:3px;
}

