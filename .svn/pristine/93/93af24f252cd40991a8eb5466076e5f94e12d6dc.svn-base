/*Media Queries*/
@media only screen
and (min-width : 266px)
{
  .mainContentBlock {
    width: 1200px;
    background-color: white;
    margin-left: calc((100% - 1200px)/2);
    height: 510px;
  }
}

@media only screen
and (min-width : 404px)
{
  .mainContentBlock {
    width: 1200px;
    background-color: white;
    margin-left: calc((100% - 1200px)/2);
    height: 510px;
  }
}

@media only screen
and (min-width : 480px)
{
  .mainContentBlock {
    width: 450px;
    background-color: white;
    margin-left: calc((100% - 450px)/2);
    height: 510px;
  }
}

 @media only screen
 and (min-width : 768px)
 {
   .mainContentBlock {
     width: 698px;
     background-color: white;
     margin-left: calc((100% - 698px)/2);
     height: 510px;
   }
 }

 @media only screen
 and (min-width : 992px)
 {
   .mainContentBlock {
     width: 922px;
     background-color: white;
     margin-left: calc((100% - 922px)/2);
     height: 510px;
   }
 }

 @media only screen
 and (min-width : 1200px)
 {
   .mainContentBlock {
     width: 1130px;
     background-color: white;
     margin-left: calc((100% - 1130px)/2);
     height: 510px;
   }
 }

 @media only screen
 and (min-width : 1410px)
 {
   .mainContentBlock {
     width: 1340px;
     background-color: white;
     margin-left: calc((100% - 1340px)/2);
     height: 510px;
   }
 }
