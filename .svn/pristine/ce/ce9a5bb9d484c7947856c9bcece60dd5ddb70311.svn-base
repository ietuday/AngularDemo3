@import '../../styles.scss';

/*Login Form Css For Mobile*/


.login {
  margin:0 auto;
  min-height: 460px;
  background:url("assets/banner.jpg");
  /*height: 675px;*/
  padding-top: 15%;
  width: 100%;
}
.login-header {
    color:#fff;
    text-align: center;
    font-size: 150%;
    padding-left: 25px;
    padding-right: 25px;
    padding-top: 9px;
}

.LoginEliteLogo{
    /*border: 1px solid black;*/
}
.login-form .ui-dropdown .ui-dropdown-trigger{
  /*border:.5px solid #fff;*/
  background:white;
  /*border-radius:10px;*/
  /*box-shadow:0px 0px 10px #000;*/
}
.login-form h3 {
  text-align:left;
  margin-left:40px;
  color:#fff;
}
.login-form {
  box-sizing:border-box;
  /*padding-top:15px;*/
  padding-bottom:10%;
  /*margin:5% auto;*/
  text-align:center;
  margin-left: -1%;
  padding-top: 18px;
}
.login input[type="text"],
.login input[type="password"] {
  max-width:400px;
  width: 80%;
  line-height:42px;
 font-family: 'Ubuntu', sans-serif;
  margin:3px;
  outline:none;
  padding-left:10px;
}
.login-form input[type="button"] {
  height:30px;
  width:100px;
  background:#fff;
  border:1px solid #f2f2f2;
  border-radius:20px;
  color: slategrey;
  text-transform:uppercase;
  font-family: 'Ubuntu', sans-serif;
  cursor:pointer;
}
.sign-up{
  color:#f2f2f2;
  margin-left:-70%;
  cursor:pointer;
  text-decoration:underline;
}
.no-access {
  color:#E86850;
  margin:20px 0px 20px -57%;
  text-decoration:underline;
  cursor:pointer;
}
.try-again {
  color:#f2f2f2;
  text-decoration:underline;
  cursor:pointer;
}

.buttonLogin {
  position: relative;
  display: inline-block;
  padding: 12px 24px;
  margin: .3em 0 1em 0;
  width: 83%;
  vertical-align: middle;
  color: #fff;
  font-size: 12px;
  line-height: 20px;
  text-align: center;
  letter-spacing: 1px;
  background: transparent;
  border: 0;
  height: 46px;
  margin-top: 16px;
  cursor: pointer;
  -webkit-transition:all 0.15s ease;
  transition: all 0.15s ease;
}

.buttonRed {
  background: $emaroon;
  // text-shadow: 1px 1px 0 rgba(39, 110, 204, .5);
}

.LoginHeading1{
    height: 25px;
    padding-top: 16px;
}

.login-form .ui-dropdown{
    max-width: 400px !important;
    width: 80% !important;
    line-height: 3em !important;
    font-family: 'Ubuntu', sans-serif !important;
    margin: 1em 2em !important;
    border-radius: 5px !important;
    border: 2px solid #f2f2f2 !important;
    outline: none !important;
    padding-left: 10px !important;
}

/*Media Querie*/
@media only screen and (min-width : 150px) and (max-width : 530px){
  .login-form h3 {
    text-align:center;
    margin:0;
  }
  .sign-up, .no-access {
    margin:10px 0;
  }
  .login-button {
    margin-bottom:10px;
  }
}































