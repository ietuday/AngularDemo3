<div class="login">
  <div class="login-header">
      <div class="LoginEliteLogo">
          <img src="assets/logo.png" style="width: 35%;">
      </div>
   <div class="LoginHeading1">
       <div style="float: left;margin-left: 20%;"><span style="color: rgb(146, 27, 49);font-weight: bolder;">Elite's</span> History</div>
      </div>
      <div style="float: left;width: 100%;margin-left: -6%;">& Corporate</div>
      <div style="text-align: center">Vision & Mission</div>
  </div>
<div class="login-form">
    <input type="text" pInputText placeholder="User Id"/>
    <br>
    <input type="password"pInputText placeholder="Password"/>
    <br>
     <p-dropdown [options]="customers" [(ngModel)]="selectCustomer" name="selectCustomer" [style]="{'max-width': '400px','width':'80%','line-height':'3em','margin':'1em 2em','border-radius':'5px','border':'2px solid #f2f2f2','outline':'none','padding-left':'10px'}"></p-dropdown>
    <br>
     <button type="button" class="buttonLogin buttonRed" (click)="login()">SIGN IN</button>
</div>
</div>




















