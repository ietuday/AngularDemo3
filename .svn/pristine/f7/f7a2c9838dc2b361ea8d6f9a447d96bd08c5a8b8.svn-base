<!--*ngIf= enabFooter-->
<div class="mobileFooter">
    <div>
        <div class="mobileFooterCreated"><i class="fa fa-pencil-square-o mobileFooterIconCreated" aria-hidden="true"></i>Created</div>
        <div class="mobileFooterBlanck">&nbsp;</div>       
        <div class="mobileFooterBooked"><i class="fa fa-square mobileFooterIconBooked" aria-hidden="true"></i>Booked</div>
        <div class="mobileFooterBlanck">&nbsp;</div>
        <div class="mobileFooterpacked"><i class="fa fa-square mobileFooterIconPacked" aria-hidden="true"></i>Packed</div>
        <div class="mobileFooterBlanck">&nbsp;</div>
        <div class="mobileFooterManifest"><i class="fa fa-square mobileFooterIconManifest" aria-hidden="true"></i>Manifest</div>
    </div>

</div>
